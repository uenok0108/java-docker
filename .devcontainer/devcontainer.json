{
  "name": "java-docker",
  "dockerComposeFile": [
    "../docker-compose.yml"
  ],
  "forwardPorts": [8080],
  "service": "tomcat",
  "workspaceFolder": "/usr/java",
  "extensions": [
    "redhat.vscode-community-server-connector",
    "vscjava.vscode-java-pack"
  ],
  "settings": {
    "java.configuration.runtimes": [
      {
        "name": "JavaSE-1.8",
        "path": "/usr/lib/jvm/java-1.8.0-amazon-corretto",
        "default": true
      },
      {
        "name": "JavaSE-11",
        "path": "/usr/lib/jvm/java-11-amazon-corretto"
      }
    ],
    "java.debug.settings.hotCodeReplace": "auto",
    "java.home": "/usr/lib/jvm/java-1.8.0-amazon-corretto"
  }
}